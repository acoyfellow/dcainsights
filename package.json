{
  "name": "dcainsights",
  "version": "0.0.1",
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.3",
    "@sveltejs/kit": "^2.43.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.13",
    "date-fns": "^4.1.0",
    "lucide-svelte": "^0.544.0",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "uplot": "^1.6.32",
    "vite": "^7.1.7",
    "wrangler": "^4.57.0"
  },
  "private": true,
  "scripts": {
    "kill": "fuser -k 8080/tcp || lsof -ti tcp:8080 | xargs --no-run-if-empty kill -9",
    "dev": "clear && bun kill &&vite dev --host --port 8080",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "type": "module",
  "dependencies": {
    "stripe": "^20.1.1"
  }
}
